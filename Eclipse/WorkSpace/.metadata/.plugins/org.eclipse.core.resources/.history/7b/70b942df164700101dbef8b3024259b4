public class ConversaoGal2Lit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double galao = 10;
		double litros = 0;
		
		//Fazendo a conversão e mostrando
		System.out.println(galao + " galões é igual a " + litros + " litros");
	}

}
